
public class StackTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		LinkStack<Integer> test1 = new LinkStack();
		test1.push(1);
		System.out.println("The last value after push 1 into the stack is "+test1.top());
		System.out.println("After push 1 into the stack, is this stack empty?\n"+test1.isEmpty());
		System.out.println("After push 1 into the stack, the size of this stack is "+test1.size());
		test1.push(2);
		System.out.println("The no argument constructor has value "+test1.top());
		System.out.println("After push 1 into the stack, is this stack empty?\n"+test1.isEmpty());
		System.out.println("After push 1 into the stack, the size of this stack is "+test1.size());
		test1.push(3);
		System.out.println("The no argument constructor has value "+test1.top());
		System.out.println("After push 1 into the stack, is this stack empty?\n"+test1.isEmpty());
		System.out.println("After push 1 into the stack, the size of this stack is "+test1.size());
		test1.pop();
		System.out.println("After pop this stack once, the last value is"+test1.top());
		System.out.println("After pop this stack once, is this stack empty?\n "+test1.isEmpty());
		test1.pop();
		System.out.println("After pop this stack twice, the last value is"+test1.top());
		System.out.println("After pop this stack twice, is this stack empty?\n "+test1.isEmpty());
		test1.pop();
		System.out.println("After pop this stack three times, the last value is"+test1.top());
		System.out.println("After pop this stack three times, is this stack empty?\n "+test1.isEmpty());
		test1.pop();
		System.out.println("After pop this stack four times(more than the size), the last value is"+test1.top());
		System.out.println("After pop this stack four times(more than the size), is this stack empty?\n "+test1.isEmpty());
		
		LinkStack<String> test2 = new LinkStack();
		test2.push("A");
		System.out.println("The last value after push A into the stack is  "+test2.top());
		System.out.println("After push A into the stack, is this stack empty?\n "+test2.isEmpty());
		System.out.println("After push A into the stack, the size of this stack is  "+test2.size());
		test2.push("B");
		System.out.println("The last value after push B into the stack is  "+test2.top());
		System.out.println("After push B into the stack, is this stack empty?\n "+test2.isEmpty());
		System.out.println("After push B into the stack, the size of this stack is  "+test2.size());
		test2.push("C");
		System.out.println("The last value after push C into the stack is  "+test2.top());
		System.out.println("After push C into the stack, is this stack empty?\n "+test2.isEmpty());
		System.out.println("After push C into the stack, the size of this stack is  "+test2.size());
		test2.pop();
		System.out.println("After pop this stack once, the last value is "+test2.top());
		System.out.println("After pop this stack once, is this stack empty?\n "+test2.isEmpty());
		System.out.println("After pop this stack once, the size of this stack is "+test2.size());
		test2.pop();
		System.out.println("After pop this stack twice, the last value is "+test2.top());
		System.out.println("After pop this stack twice, is this stack empty?\n "+test2.isEmpty());
		System.out.println("After pop this stack twice, the size of this stack is "+test2.size());
		test2.pop();
		System.out.println("After pop this stack three times, the last value is "+test2.top());
		System.out.println("After pop this stack three times, is this stack empty?\n "+test2.isEmpty());
		System.out.println("After pop this stack three times, the size of this stack is "+test2.size());
		test2.pop();
		System.out.println("After pop this stack four times, the last value is "+test2.top());
		System.out.println("After pop this stack four times, is this stack empty?\n "+test2.isEmpty());
		System.out.println("After pop this stack four times, the size of this stack is "+test2.size());
		ArraylistStack<String> test3 = new ArraylistStack();
		test3.push("A");
		System.out.println("The last element in the list is "+test3.top());
		System.out.println("Is this list empty? \nThen answer is "+test3.isEmpty());
		System.out.println("The size of this list is "+test3.size());
		test3.push("B");
		System.out.println("The last element in the list is "+test3.top());
		System.out.println("Is this list empty? \nThen answer is "+test3.isEmpty());
		System.out.println("The size of this list is "+test3.size());
		test3.push("C");
		System.out.println("The last element in the list is "+test3.top());
		System.out.println("Is this list empty? \nThen answer is "+test3.isEmpty());
		System.out.println("The size of this list is "+test3.size());
		test3.pop();
		System.out.println("The last element after pop that once is "+test3.top());
		System.out.println("Is this list empty? \nThen answer is "+test3.isEmpty());
		System.out.println("The size of this list is "+test3.size());
		test3.pop();
		System.out.println("The last element after pop that twice is "+test3.top());
		System.out.println("Is this list empty? \nThen answer is "+test3.isEmpty());
		System.out.println("The size of this list is "+test3.size());
		test3.pop();
		System.out.println("The last element after pop that three times is "+test3.top());
		System.out.println("Is this list empty? \nThen answer is "+test3.isEmpty());
		System.out.println("The size of this list is "+test3.size());
		
		test3.pop();
		System.out.println("The last element after pop that four (more than its length) times is "+test3.top());
		System.out.println("Is this list empty? \nThen answer is "+test3.isEmpty());
		System.out.println("The size of this list is "+test3.size());

	}

}
